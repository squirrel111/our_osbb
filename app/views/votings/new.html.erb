<h1>Create vote</h1>
<%= form_for :voting, url: user_votings_path(current_user) do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :start_date%><br>
    <%= f.date_field :start_date%>
  </p>

  <p>
    <%= f.label :end_date%><br>
    <%= f.date_field :end_date%>
  </p>
  
  <p>
    <%= f.submit 'Create vote'%>
  </p>
<% end %>

<%= link_to 'Back', user_path(current_user.id)%>
